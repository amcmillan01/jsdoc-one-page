{
  "name": "jsdoc-one-page",
  "version": "0.0.3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amcmillan01/jsdoc-one-page.git"
  },
  "author": "Andrei McMillan",
  "description": "A one page jsdoc template based on the default jsdoc template.",
  "bin": {
    "jsdoc1p": "./node_modules/jsdoc/jsdoc.js"
  },
  "keywords": [
    "jsdoc-one-page",
    "jsdoc",
    "template",
    "jsdoc1p"
  ],
  "scripts": {
    "prepare": "npm install && mkdir -p output",
    "doc": "jsdoc -t . -r ./node_modules/jsdoc/lib/jsdoc -d output",
    "test": "echo 'write some tests!'"
  },
  "dependencies": {
    "jsdoc": "^3.5.4",
    "taffydb": "^2.7.3"
  }
}
